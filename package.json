{
  "name": "ts-typebench",
  "displayName": "ts-typebench",
  "description": "TypeBox Workbench integrated into VsCode",
  "version": "0.0.8",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "publisher": "typeholes",
  "repository": "https://github.com/typeholes/ts-typebench",
  "activationEvents": [],
  "main": "./out/main.js",
  "browser": "./out/main.js",
  "contributes": {
    "configuration": {
      "title": "TypeBench",
      "properties": {
        "TypeBench.maxTypes": {
          "default": 100,
          "type": "number",
          "description": "guards against type extraction going off the rails"
        },
        "TypeBench..default.extractor": {
          "type": "string",
          "enum": [
            "ArkType",
            "IoTs",
            "TypeScript",
            "Javascript",
            "JsonSchema",
            "TypeBox",
            "SampleValue",
            "Zod",
            "Valibot",
            "Identity"
          ],
          "default": "Identity"
        },
        "TypeBench.extractor.ArkType": {
          "type": "object",
          "default": {
            "target": "clipboard",
            "language": "typescript",
            "commentOldTypes": false,
            "autoUndo": false
          },
          "additionalProperties": false,
          "required": [
            "target",
            "language",
            "commentOldTypes",
            "autoUndo"
          ],
          "properties": {
            "commentOldTypes": {
              "type": "boolean"
            },
            "target": {
              "type": "string",
              "enum": [
                "clipboard",
                "inplace",
                "newDocument"
              ]
            },
            "language": {
              "type": "string",
              "enum": [
                "typescript",
                "javascript",
                "json",
                "text"
              ]
            },
            "autoUndo": {
              "type": "boolean"
            }
          }
        },
